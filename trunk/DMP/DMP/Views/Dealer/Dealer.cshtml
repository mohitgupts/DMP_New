@model DMP.ModelsView.CsmUserViewModel
@{
    ViewBag.Title = "Dealer";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style type="text/css">
    .row1
    {
        background-color: #f6f6f6 !important;
    }
    .head1
    {
        background-color: #c21715;
        color: white;
        font-weight: bold;
    }
    .img1
    {
        padding-left: 10px !important;
    }
</style>
<div class="row-fluid">
    <div class="span12">
        @if (ViewBag.UserName != null) {
            <h3 style="text-align: center; font-weight: bold; color: red; background-color: #aaaaaa">@ViewBag.UserName</h3>
        }
    </div>
</div>
<div class="row-fluid">
    <div class="span3">
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th class="head1">
                        <img src="../../Images/Reports.png" />
                        Reports
                    </th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td class="row1">
                        <img src="../../Images/trend_report.png" class="img1" />
                        <a href="#">DSM Retail Trend</a>
                    </td>
                </tr>
                <tr>
                    <td class="row1">
                        <img src="../../Images/Dashboard.png" class="img1" />
                        <a href="#">DSM Dashbaoard</a>
                    </td>
                </tr>
                <tr>
                    <td class="row1">
                        <img src="../../Images/incentive.png" class="img1" />
                        <a href="#">DSM Incentive</a>
                    </td>
                </tr>
                <tr>
                    <td class="row1">
                        <img src="../../Images/attrition.png" class="img1" />
                        <a href="#">DSM Attrition Analysis</a>
                    </td>
                </tr>
                <tr>
                    <td class="row1">
                        <img src="../../Images/productivity.png" class="img1" />
                        <a href="#">DSM Wise Productivity</a>
                    </td>
                </tr>
                <tr>
                    <td class="row1">
                        <img src="../../Images/competence.png" class="img1" />
                        <a href="#">DSM Wise Competence</a>
                    </td>
                </tr>
                <tr>
                    <td class="row1">
                        <img src="../../Images/trend_report.png" class="img1" />
                        <a href="#">DSE Retail Trend</a>
                    </td>
                </tr>
                <tr>
                    <td class="row1">
                        <img src="../../Images/Dashboard.png" class="img1" />
                        <a href="#">DSE Dashbaoard</a>
                    </td>
                </tr>
                <tr>
                    <td class="row1">
                        <img src="../../Images/incentive.png" class="img1" />
                        <a href="#">DSE Incentive</a>
                    </td>
                </tr>
                <tr>
                    <td class="row1">
                        <img src="../../Images/attrition.png" class="img1" />
                        <a href="#">DSE Attrition Analysis</a>
                    </td>
                </tr>
                <tr>
                    <td class="row1">
                        <img src="../../Images/productivity.png" class="img1" />
                        <a href="#">DSE Wise Productivity</a>
                    </td>
                </tr>
                <tr>
                    <td class="row1">
                        <img src="../../Images/competence.png" class="img1" />
                        <a href="#">DSE Wise Competence</a>
                    </td>
                </tr>
            </tbody>
        </table>
        @if (User.IsInRole("CSM")) {
            <table class="table table-bordered">
                <thead>
                    <tr>
                        <th class="head1">
                            <img src="../../Images/tools.png" />
                            Tools
                        </th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td class="row1">
                            <img src="../../Images/add.png" class="img1" />
                            <a href="/Dealer/ManpowerProfile?id=0&dealerId=@ViewBag.dealerId" id="linkMnanageDse">
                                Add DSE</a>
                        </td>
                    </tr>
                    <tr>
                        <td class="row1">
                            <img src="../../Images/user_manage.png" class="img1" />
                            <a href="/Dealer/ManageDse?csmId=@ViewBag.csmId&dealerId=@ViewBag.dealerId" id="linkMnanageDse">
                                Manage DSE</a>
                        </td>
                    </tr>
                    <tr>
                        <td class="row1">
                            <img src="../../Images/target.png" class="img1" />
                            <a href="/Dealer/Targets/@ViewBag.DealerId" id="linkTarget">Targets</a>
                        </td>
                    </tr>
                    <tr>
                        <td class="row1">
                            <img src="../../Images/incentive.png" class="img1" />
                            <a href="/Incentive/Incentive/@ViewBag.DealerId" id="linkIncentive">Incentives</a>
                        </td>
                    </tr>
                    @*<tr>
                    <td class="row1">
                        <img src="../../Images/file_upload.png" class="img1" />
                        <a href="/Dealer/UploadFile" id="linkFileUpload">Upload File</a>
                    </td>
                </tr>*@
                </tbody>
            </table>
        }
    </div>
    <div class="span9">
        <div>
            @Html.Partial("ReportManpowerPartial", Model.ReportManpower)
        </div>
        <div>
            @Html.Partial("ProductStatPartial", Model.ProductStatistics)
        </div>
    </div>
</div>
